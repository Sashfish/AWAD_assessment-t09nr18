<p id="notice"><%= notice %></p>

<h1>Properties</h1>

<table>
  <thead>
    <tr>
      <th>Suburb</th>
      <th>Address</th>
      <th>Type</th>
      <th>Price</th>
      <th>Yearbuilt</th>
      <th>Councilarea</th>
      <th>Lattitude</th>
      <th>Longitude</th>
      <th>Regionname</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.suburb %></td>
        <td><%= property.address %></td>
        <td><%= property.type %></td>
        <td><%= property.price %></td>
        <td><%= property.yearbuilt %></td>
        <td><%= property.councilarea %></td>
        <td><%= property.lattitude %></td>
        <td><%= property.longitude %></td>
        <td><%= property.regionname %></td>
        <td><%= link_to 'Show', property %></td>
        <td><%= link_to 'Edit', edit_property_path(property) %></td>
        <td><%= link_to 'Destroy', property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property', new_property_path %>
